<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.colourit.gui.Controller_ProjectList">
    <children>
        <GridPane prefHeight="115.0" prefWidth="1280.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="25.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Projects">
                    <font>
                        <Font name="System Bold" size="20.0" />
                    </font>
                    <GridPane.margin>
                        <Insets left="20.0" />
                    </GridPane.margin>
                </Text>
                <Button fx:id="createButton" mnemonicParsing="false" onMouseClicked="#createProjectButton" prefHeight="21.0" prefWidth="93.0" text="Create" GridPane.columnIndex="1">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="backButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#loginScreen" prefHeight="21.0" prefWidth="93.0" text="Back" GridPane.columnIndex="2">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="addEmployeeButton" alignment="CENTER" mnemonicParsing="false" onMouseClicked="#addEmployee" prefHeight="21.0" prefWidth="160.0" text="Add Employee" GridPane.columnIndex="3">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
                <Button fx:id="exportButton" mnemonicParsing="false" onAction="#exportToWebsite" prefHeight="39.0" prefWidth="184.0" text="Export to website" GridPane.columnIndex="4">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Button>
            </children>
        </GridPane>
        <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="605.0" prefWidth="1280.0">
            <children>
                <TableView fx:id="projectTableView" onMouseClicked="#itemSelected" prefHeight="605.0" prefWidth="807.0">
                    <columns>
                        <TableColumn fx:id="projectName" prefWidth="266.0" text="Project name" />
                        <TableColumn fx:id="startDate" prefWidth="112.7999267578125" text="Start Date" />
                        <TableColumn fx:id="deadLine" minWidth="2.0" prefWidth="128.0" text="Deadline" />
                        <TableColumn fx:id="projectStatus" minWidth="0.0" prefWidth="132.0" text="Status" />
                    </columns>
                </TableView>
                <TableView fx:id="employeeTableView" prefHeight="200.0" prefWidth="640.0">
                    <columns>
                        <TableColumn fx:id="employeeNameColumn" prefWidth="247.0" text="Name" />
                        <TableColumn fx:id="idNumberColumn" minWidth="0.0" prefWidth="93.0" text="ID Number" />
                        <TableColumn fx:id="birthdayColumn" prefWidth="124.0" text="Birthday" />
                    </columns>
                </TableView>
            </children>
        </HBox>
      <HBox>
         <children>
              <Label text="Colour IT Â© Group 7" textFill="#545454">
                  <font>
                      <Font name="System Italic" size="12.0" />
                  </font>
               <HBox.margin>
                  <Insets left="10.0" right="470.0" />
               </HBox.margin>
              </Label>
            <Label fx:id="roleSelectedLabel" text="Role Selected">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Label>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
    </children>

</VBox>
