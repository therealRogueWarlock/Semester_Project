<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.colourit.gui.Controller_RequirementList">
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
        <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
        </padding>
        <Label fx:id="projectNameLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Project Name">
            <padding>
                <Insets left="15.0" right="15.0" />
            </padding>
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Label>
        <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="6.0" />
        <Label fx:id="statusLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Status">
            <padding>
                <Insets left="15.0" right="15.0" />
            </padding>
        </Label>
        <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="6.0" />
        <Label fx:id="deadlineLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="46.0" prefWidth="150.0" text="Deadline">
            <padding>
                <Insets left="15.0" right="15.0" />
            </padding>
        </Label>
        <HBox alignment="CENTER_RIGHT" prefHeight="46.0" prefWidth="516.0" spacing="10.0" HBox.hgrow="ALWAYS">
            <HBox prefHeight="46.0" prefWidth="385.0">
                <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="6.0" />
                <Label fx:id="totalTimeSpentLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Total time spent" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets left="15.0" right="15.0" />
                    </padding>
                </Label>
                <Separator orientation="VERTICAL" prefHeight="46.0" prefWidth="0.0" />
            </HBox>
            <Button fx:id="editButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#editButton" prefHeight="46.0" prefWidth="133.0" text="Edit Project" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets left="15.0" right="15.0" />
                </padding>
            </Button>
            <Button fx:id="backButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#goBack" prefHeight="46.0" prefWidth="113.0" text="Back" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets left="15.0" right="15.0" />
                </padding>
            </Button>
            <Button fx:id="deleteProjectButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#deleteProjectButton" prefHeight="46.0" prefWidth="113.0" text="Delete Project" visible="false" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets left="15.0" right="15.0" />
                </padding>
            </Button>
        </HBox>
    </HBox>
    <SplitPane dividerPositions="0.5" focusTraversable="true" VBox.vgrow="ALWAYS">
        <VBox prefHeight="200.0" prefWidth="100.0">
            <GridPane prefHeight="98.0" prefWidth="446.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="102.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="234.0" minWidth="10.0" prefWidth="189.0" />
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="196.0" minWidth="10.0" prefWidth="155.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                </rowConstraints>
                <Label text="Requirements">
                    <GridPane.margin>
                        <Insets left="5.0" />
                    </GridPane.margin>
                    <font>
                        <Font name="System Bold" size="15.0" />
                    </font>
                </Label>
                <Button fx:id="addRequirementButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#addRequirementButton" text="Add Requirement" GridPane.columnIndex="2">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </Button>
                <ChoiceBox fx:id="requirementChoiceBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </ChoiceBox>
                <Button fx:id="removeRequirementButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMouseClicked="#removeRequirementButton" text="Remove Requirement" GridPane.columnIndex="2" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </GridPane.margin>
                </Button>
            </GridPane>
            <TableView fx:id="requirementTable" onMouseClicked="#itemSelected" prefHeight="550.0" prefWidth="636.0">
                <columns>
                    <TableColumn fx:id="requirementPriorityColumn" maxWidth="1.7976931348623157E308" prefWidth="57.0" text="Priority" />
                    <TableColumn fx:id="requirementNameColumn" prefWidth="274.0" text="Name" />
                    <TableColumn fx:id="requirementStatusColumn" prefWidth="141.0" text="Status" />
               <TableColumn fx:id="requirementTimeEstimateColumn" prefWidth="163.0" text="Time Estimate" />
                </columns>
            </TableView>
        </VBox>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <SplitPane dividerPositions="0.5" orientation="VERTICAL">
                <AnchorPane maxHeight="-Infinity" prefHeight="90.0" prefWidth="492.0">
                    <HBox prefHeight="45.0" prefWidth="492.0" spacing="10.0">
                        <padding>
                            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
                        </padding>
                        <ChoiceBox fx:id="addTeamMemberChoiceBox" prefWidth="150.0">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </ChoiceBox>
                        <ChoiceBox fx:id="selectRoleChoiceBox" prefHeight="25.0" prefWidth="84.0">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </ChoiceBox>
                        <Button fx:id="addTeamMemberButton" mnemonicParsing="false" onMouseClicked="#addTeamMemberButton" prefHeight="25.0" prefWidth="153.0" text="Add Team Member ">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Button>
                    </HBox>
                    <HBox layoutY="45.0" prefHeight="45.0" prefWidth="492.0">
                        <ChoiceBox fx:id="removeTeamMemberChoiceBox" prefWidth="150.0">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </ChoiceBox>
                        <Button fx:id="removeTeamMemberButton" mnemonicParsing="false" onMouseClicked="#removeTeamMemberButton" prefHeight="25.0" prefWidth="112.0" text="Remove Team Member">
                            <HBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </HBox.margin>
                        </Button>
                    </HBox>
                </AnchorPane>
                <TableView fx:id="teamMemberTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="578.0" prefWidth="446.0">
                    <columns>
                        <TableColumn fx:id="teamMemberNameColumn" prefWidth="192.0" text="Team Member Name" />
                        <TableColumn fx:id="idNumberColumn" prefWidth="137.0" text="ID Number" />
                        <TableColumn fx:id="roleColumn" minWidth="0.0" prefWidth="116.0" text="Role" />
                    </columns>
                </TableView>
            </SplitPane>
        </VBox>
    </SplitPane>
   <HBox>
      <children>
           <Label text="Colour IT Â© Group 7" textFill="#545454">
               <font>
                   <Font name="System Italic" size="12.0" />
               </font>
            <HBox.margin>
               <Insets right="480.0" />
            </HBox.margin>
           </Label>
         <Label fx:id="roleSelectedLabel" text="Role Selected" />
      </children>
   </HBox>

</VBox>
