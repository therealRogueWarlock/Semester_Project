<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="dk.colourit.gui.SecondaryController">
    <children>
        <Label text="More Options"/>
        <Button fx:id="addMemberButton" layoutX="30.0" layoutY="67.0" onAction="#switchToAddTeamMember"
                text="Add Member"/>
        <Button fx:id="taskListButton" onAction="#switchToTaskList" text="Task List"/>
        <Button fx:id="backButton" onAction="#switchToPrimary" text="Back to Main View"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
